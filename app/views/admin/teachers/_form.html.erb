<%= form_for([:admin, @teacher]) do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <%= f.label :role %>
  <%= f.number_field :role %>
  <%= f.submit "Create or Update Account" %>
<% end %>
